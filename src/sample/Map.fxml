<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="420.0" style="-fx-background-color: #9FE1B1;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MapController">
   <children>
      <ImageView fitHeight="511.0" fitWidth="402.0" layoutX="9.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/maphc2.png" />
         </image></ImageView>
      <ButtonBar fx:id="summary" layoutX="183.0" layoutY="224.0" prefHeight="47.0" prefWidth="217.0" style="-fx-border-color: #9FE1B1;" visible="false">
        <buttons>
            <Label text="Modern university venue serving drinks and comfort food as well as hosting gigs and events." wrapText="true">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
          <Button fx:id="btnSum" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="31.0" mnemonicParsing="false" onAction="#showDeal" prefHeight="39.0" prefWidth="30.0" style="-fx-background-color: transparent; -fx-border-color: #9FE1B1;" text="&gt;" textFill="#9fe1b1">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
      <Button fx:id="drapersIcon" layoutX="168.0" layoutY="185.0" mnemonicParsing="false" onAction="#draperSummary" prefHeight="32.0" prefWidth="29.0" style="-fx-background-color: transparent;" />
      <Button fx:id="btnReturn" layoutX="21.0" layoutY="533.0" mnemonicParsing="false" onAction="#returnLogin" prefHeight="53.0" prefWidth="93.0" style="-fx-background-color: WHITE;" text="&lt;" textFill="#9fe1b1">
         <font>
            <Font size="24.0" />
         </font></Button>
   </children>
</AnchorPane>
